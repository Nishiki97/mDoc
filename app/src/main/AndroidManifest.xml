<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.industrialmaster.mdoc">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DocHomeActivity" />
        <activity
            android:name=".MyPatientsActivity"
            android:label="@string/title_activity_my_patients"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MyPlacesOfPractice"
            android:label="@string/title_activity_my_places_of_practice"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".AddPlaces" />
        <activity android:name=".AddConsultationHours" />
        <activity android:name=".DocProfile" />
        <activity android:name=".QualificationActivity"></activity>
    </application>

</manifest>